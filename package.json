{
  "name": "team7",
  "version": "1.0.0",
  "description": "financial management website that lets users enter their expenses and provides personalized budgeting recommendations.",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently --kill-others \"cd server && npm run dev\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "seed": "cd server && npm run seed",
    "deploy-build": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezizbagshiyev/Team7.git"
  },
  "keywords": [
    "jopa"
  ],
